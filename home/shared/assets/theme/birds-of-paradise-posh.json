{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "background": "#6b98bb",
                    "foreground": "#372725",
                    "options": {
                        "style": "austin",
                        "threshold": 321
                    },
                    "style": "plain",
                    "template": " {{ .FormattedMs }} ",
                    "type": "executiontime"
                },
                {
                    "background": "#A40042",
                    "foreground": "#E6E1C4",
                    "style": "plain",
                    "template": "{{ if gt .Code 0 }} {{ .Code }} {{ end }}",
                    "type": "status"
                },
                {
                    "background": "#EFCB43",
                    "foreground": "#372725",
                    "style": "plain",
                    "template": "\uf0e7",
                    "type": "root"
                },
                {
                    "background": "#372725",
                    "foreground": "#6b98bb",
                    "style": "plain",
                    "template": " {{ if .WSL }}\uebcc {{ end }}{{.Icon}}",
                    "type": "os"
                },
                {
                    "background": "#372725",
                    "foreground": "#E6E1C4",
                    "style": "plain",
                    "template": " {{ if .SSHSession }}\ueba9 {{ end }}{{ .UserName }}@{{ .HostName }} ",
                    "type": "session"
                },
                {
                    "background": "#5B413D",
                    "foreground": "#E6E1C4",
                    "options": {
                        "folder_icon": "\u2026",
                        "style": "mixed"
                    },
                    "style": "plain",
                    "template": " {{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "#6ba18a",
                    "foreground": "#372725",
                    "options": {
                        "branch_icon": "\ue725 ",
                        "cherry_pick_icon": "\ue29b ",
                        "commit_icon": "\uf417 ",
                        "fetch_status": false,
                        "fetch_upstream_icon": false,
                        "merge_icon": "\ue727 ",
                        "no_commits_icon": "\uf0c3 ",
                        "rebase_icon": "\ue728 ",
                        "revert_icon": "\uf0e2 ",
                        "tag_icon": "\uf412 "
                    },
                    "style": "plain",
                    "template": " {{ .HEAD }} ",
                    "type": "git"
                },
                {
                    "background": "#bb94b4",
                    "foreground": "#372725",
                    "options": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": " \ue77f ",
                    "type": "dotnet"
                },
                {
                    "background": "#85b4bb",
                    "foreground": "#372725",
                    "options": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": " \ue626 ",
                    "type": "go"
                },
                {
                    "background": "#eeac36",
                    "foreground": "#372725",
                    "options": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": " \ue235 ",
                    "type": "python"
                },
                {
                    "background": "#cb4131",
                    "foreground": "#E6E1C4",
                    "options": {
                        "fetch_version": false
                    },
                    "style": "plain",
                    "template": " \ue7a8 ",
                    "type": "rust"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [
                {
                    "background": "#372725",
                    "foreground": "#6b98bb",
                    "style": "plain",
                    "template": " {{ .CurrentDate |date .Format }} ",
                    "type": "time",
                    "options": {
                        "time_format": "15:04:05 MEZ"
                    }
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#6b98bb",
                    "style": "plain",
                    "template": "{{ .Name }} ",
                    "type": "shell",
                    "options": {
                        "mapped_shell_names": {
                            "pwsh": "\u276f",
                            "shell": "\u276f",
                            "cmd": ">",
                            "lua": ">",
                            "nu": ":)",
                            "fish": "~>",
                            "zsh": "%",
                            "bash": "$"
                        }
                    }
                }
            ],
            "type": "prompt"
        }
    ],
    "version": 4
}